{
  "name": "aadish-server",
  "version": "1.1.0",
  "description": "A terminal-based chat client for the Akatsuki chat server, providing a rich CLI user experience.",
  "author": "Aadish",
  "license": "MIT",
  "type": "module",
  "main": "src/index.jsx",
  "bin": {
    "aadish": "./src/index.jsx"
  },
  "scripts": {
    "build": "esbuild src/index.jsx --bundle --platform=node --outfile=dist/index.cjs --format=cjs",
    "start": "tsx src/index.jsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/your-repo.git"
  },
  "keywords": [
    "chat",
    "cli",
    "terminal",
    "ink",
    "react",
    "websocket",
    "akatsuki"
  ],
  "dependencies": {
    "chalk": "^5.5.0",
    "ink": "^6.1.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "esbuild": "^0.25.8",
    "tsx": "^4.20.3"
  }
}
