{
  "name": "aadish-server",
  "version": "1.1.8",
  "description": "A terminal-based chat client for the Akatsuki chat server, providing a rich CLI user experience.",
  "author": "Aadish",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.mjs",
  "bin": {
    "aadish": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esbuild src/index.jsx --bundle --platform=node --outfile=dist/index.mjs --format=esm --external:react --external:chalk --external:ink --external:ink-spinner --external:ink-text-input --external:uuid --external:ws",
    "start": "tsx src/index.jsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AadishY/Server.git"
  },
  "keywords": [
    "chat",
    "cli",
    "terminal",
    "ink",
    "react",
    "websocket",
    "akatsuki"
  ],
  "dependencies": {
    "chalk": "^5.5.0",
    "ink": "^6.1.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "esbuild": "^0.25.8",
    "react-devtools-core": "^4.19.1",
    "tsx": "^4.20.3"
  }
}
